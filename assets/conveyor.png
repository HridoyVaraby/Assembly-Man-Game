<svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" viewBox="0 0 100 100">
  <!-- Conveyor belt background -->
  <rect width="100" height="100" fill="#2D3748"/>
  
  <!-- Conveyor belt rollers -->
  <rect x="0" y="20" width="100" height="5" fill="#4A5568"/>
  <rect x="0" y="75" width="100" height="5" fill="#4A5568"/>
  
  <!-- Conveyor belt segments -->
  <rect x="0" y="25" width="20" height="50" fill="#718096"/>
  <rect x="20" y="25" width="20" height="50" fill="#4A5568"/>
  <rect x="40" y="25" width="20" height="50" fill="#718096"/>
  <rect x="60" y="25" width="20" height="50" fill="#4A5568"/>
  <rect x="80" y="25" width="20" height="50" fill="#718096"/>
  
  <!-- Conveyor belt highlights -->
  <rect x="0" y="30" width="20" height="2" fill="#A0AEC0" opacity="0.3"/>
  <rect x="40" y="30" width="20" height="2" fill="#A0AEC0" opacity="0.3"/>
  <rect x="80" y="30" width="20" height="2" fill="#A0AEC0" opacity="0.3"/>
  
  <!-- Conveyor belt shadows -->
  <rect x="0" y="70" width="20" height="2" fill="#1A202C" opacity="0.3"/>
  <rect x="40" y="70" width="20" height="2" fill="#1A202C" opacity="0.3"/>
  <rect x="80" y="70" width="20" height="2" fill="#1A202C" opacity="0.3"/>
</svg>